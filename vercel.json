{
  "version": 2,
  "builds": [
    {
      "src": "dashboard_app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dashboard_app.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "dashboard_app.py": {
      "maxDuration": 60,
      "memory": 3008
    }
  }
}
